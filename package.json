{
  "name": "project",
  "version": "1.0.0",
  "description": "SASS compile|autoprefix|minimize and live-reload dev server using live-server for static HTML",
  "main": "index.html",
  "author": "catalinpetrovici24",
  "scripts": {
    "watch": "sass --no-source-map --watch sass/main.scss styles/style.css --style compressed",
    "dev-server": "live-server",
    "compile": "sass --no-source-map --style=compressed sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix": "postcss css/style.comp.css --use autoprefixer -o css/style.css",
    "clean": "shx rm css/style.comp.css && shx echo Done",
    "": "",
    "build": "npm-run-all compile prefix clean",
    "start": "npm-run-all --parallel watch dev-server"
  },
  "keywords": [],
  "license": "ISC",
  "browserslist": "last 10 versions",
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "sass": "^1.49.11",
    "shx": "^0.3.4"
  }
}